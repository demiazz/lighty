{
  "name": "lighty",
  "version": "0.4.2",
  "description": "Modern replacement for Evil Blocks",
  "license": "MIT",
  "homepage": "https://github.com/demiazz/lighty",
  "bugs": {
    "url": "https://github.com/demiazz/lighty/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/demiazz/lighty"
  },
  "author": {
    "name": "Alexey Plutalov",
    "email": "demiazz.py@gmail.com"
  },
  "main": "dist/lighty.js",
  "module": "dist/lighty.es.js",
  "jsnext:main": "dist/lighty.es.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "$(npm bin)/eslint src spec",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test $(npm bin)/karma start build/karma.local.js --single-run",
    "test:watch": "cross-env NODE_ENV=test $(npm bin)/karma start build/karma.local.js",
    "test:ci": "cross-env NODE_ENV=test $(npm bin)/karma start build/karma.ci.js",
    "build": "node build/rollup.js",
    "docs": "rm -rf docs && $(npm bin)/jsdoc -c .jsdocrc src/*.js",
    "version": "npm run build && npm run docs && git add -A dist && git add -A docs"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "deep-equal": "^1.0.1",
    "docdash": "^0.4.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "jasmine-core": "^2.5.2",
    "jquery": ">= 2.1.0",
    "jsdoc": "^3.4.3",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-rollup-plugin": "^0.2.4",
    "karma-sauce-launcher": "^1.1.0",
    "karma-spec-reporter": "0.0.26",
    "phantomjs-prebuilt": "^2.1.13",
    "rollup": "^0.36.3",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "uglify-save-license": "^0.4.1"
  }
}
