{
  "source": {
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "readme": "README.md",
    "template": "node_modules/docdash",
    "destination": "./docs/",
    "private": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "layoutFile": "node_modules/docdash/tmpl/layout.tmpl",
      "includeDate": false
    }
  }
}
